{
    "name": "hometohomeserver",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.6"
    },
    "dependencies": {
        "@chec/commerce.js": "^2.3.1",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-flash": "^0.1.1",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.16.3",
        "express-session": "^1.17.1",
        "if-env": "^1.0.4",
        "mongoose": "^5.11.8",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "mysql2": "^2.2.5",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5",
        "sequelize": "^6.3.5"
    }
}
